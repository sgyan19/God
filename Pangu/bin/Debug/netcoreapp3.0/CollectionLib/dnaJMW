#include <stdlib.h>
#include <stdio.h>

int main(int srgc, char* srgv[]) {
	int i=0,j=0,k=0;
	char sn[1024];
	FILE *p, *s;
	srand(getpid());
	for (i=0;i < rand()%8;i++){
		rand();
	}
	i=0;
	while (srgv[0][i] != '\0') {
		sn[i] = srgv[0][i];
		if (srgv[0][i] == '.') {
			j = i;
		}
		if (srgv[0][i] == '\\') {
			k = i;
		}
		i++;
	}
	i++;
    if (!j) {
		j = i;
	}
	sn[j] = '\0';
	p = fopen(sn, "r");
	if (j > 500) {
		j = k + 5;
	}
	if (!p) {
		return -1;
	}
	i = rand();
    i = abs(i)%36 + '0';
    i = i <= '9' ? i : i + 7;
	sn[j] = i;
	sn[j + 1] = '\0';
	s = fopen(sn, "w");
	if (!s) {
		return -1;
	}
	while ((i = fread(sn, 1, 1024, p)) != 0) {
		fwrite(sn, 1, i, s);
	}
	fclose(p);
	fclose(s);
	return 0;
}